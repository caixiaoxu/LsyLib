<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lsy.lsylib">

    <application
        android:name="com.lsy.lsylib.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lsy.lsylib.weight.SeekSelectActivity"/>
        <activity android:name="com.lsy.lsylib.weight.ZanActivity" />
        <activity android:name="com.lsy.lsylib.weight.ViewActivity" />
        <activity android:name="com.lsy.lsylib.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>